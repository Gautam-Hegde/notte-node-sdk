{
    "name": "notte-sdk",
    "version": "1.0.0",
    "description": "Node.js SDK for Notte API",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc",
        "clean": "rimraf dist",
        "dev": "ts-node -r tsconfig-paths/register src/index.ts",
        "test": "echo \"Error: no test specified\" && exit 1",
        "prepare": "npm run build",
        "start": "node dist/index.js"
    },
    "keywords": [
        "notte",
        "sdk",
        "api",
        "browser",
        "automation",
        "agent",
        "llm"
    ],
    "author": "nottelabs & Gautam Hegde",
    "license": "ISC",
    "dependencies": {
        "axios": "^1.6.5",
        "dotenv": "^16.3.1",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/node": "^20.11.0",
        "rimraf": "^5.0.5",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.3.3"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "files": [
        "dist/**/*",
        "README.md",
        "LICENSE"
    ],
    "publishConfig": {
        "access": "public"
    }
}